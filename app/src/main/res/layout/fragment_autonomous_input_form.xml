            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="High Specimens"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            app:layout_constraintBottom_toTopOf="@+id/inputFormAutonomousHighSpecimensDisplay"
            app:layout_constraintEnd_toEndOf="@+id/inputFormAutonomousHighSpecimensDisplay"
            app:layout_constraintStart_toStartOf="@+id/inputFormAutonomousHighSpecimensDisplay" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>